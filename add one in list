x=int(input("Enter the number of digit in firt list"))
print("Enter the elements of list:-")
li=[]
for i in range(x):
    s=int(input())
    li.append(s)

print(li)
n=len(li)-1
while li[n]==9:
    li[n]=0
    n-=1
if(n>-1):
    li[n]=li[n]+1
if(n==-1):
    li.insert(0,1)

print(li)
i=0
while li[i]==0:
    li.pop(0)

print(li)
